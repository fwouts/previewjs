{
  "name": "@previewjs/app",
  "version": "1.14.1",
  "license": "AGPL-3.0",
  "author": {
    "name": "Fran√ßois Wouts",
    "email": "f@zenc.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fwouts/previewjs"
  },
  "bugs": {
    "url": "https://github.com/fwouts/previewjs/issues"
  },
  "homepage": "https://previewjs.com",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./tests": {
      "require": "./dist-tests/index.js"
    }
  },
  "scripts": {
    "prepublish": "cd .. && pnpm turbo run build --scope=@previewjs/app --no-deps --include-dependencies",
    "build": "tsc && unbuild && pnpm tsc -p client/tsconfig.json && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build client && pnpm tsc -p tsconfig.tests.json",
    "client:dev": "vite client",
    "e2e-test": "tsc -p tsconfig.tests.json && pnpm previewjs-e2e-test -s ./dist/index.cjs -t ./dist-tests/index.js"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@previewjs/app-foundations": "workspace:*",
    "@previewjs/core": "workspace:*",
    "@previewjs/e2e-test-runner": "workspace:*",
    "@previewjs/plugin-react": "workspace:*",
    "@previewjs/plugin-solid": "workspace:*",
    "@previewjs/plugin-svelte": "workspace:*",
    "@previewjs/plugin-vue2": "workspace:*",
    "@previewjs/plugin-vue3": "workspace:*",
    "@types/express": "4.17.14",
    "@types/fs-extra": "9.0.13",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "fs-extra": "10.1.0",
    "mobx-react-lite": "3.4.0",
    "postcss": "8.4.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4",
    "unbuild": "0.9.1",
    "vite": "3.2.2",
    "vite-plugin-svgr": "2.2.2"
  }
}
