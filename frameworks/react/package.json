  "sideEffects": false,
  "scripts": {
    "prepublish": "cd .. && pnpm turbo run build --scope=@previewjs/plugin-react --no-deps --include-dependencies",
    "prepare": "rimraf preview/types && shx mkdir -p preview/types/react && shx cp -r preview/node_modules/@types/react/* preview/types/react/ && shx mkdir -p preview/types/prop-types && shx cp -r preview/node_modules/@types/prop-types/* preview/types/prop-types/",
    "build": "rimraf dist && tsc",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./setup": {
      "module": "./setup.esm.js",
      "default": "./setup.js"
    }
  },
  "dependencies": {
    "@previewjs/type-analyzer": "1.1.0",
    "@svgr/core": "^5.5.0",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@previewjs/config": "workspace:*",
    "@previewjs/core": "workspace:*",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.0",
    "@types/prop-types": "15.7.4",
    "@types/react": "17.0.39",
    "jest": "27.5.1",
    "next": "12.1.0",
    "rimraf": "3.0.2",
    "shx": "0.3.4",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "vite": "2.7.13"
  }
