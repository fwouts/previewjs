{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "PREVIEWJS_INTELLIJ_VERSION",
        "PREVIEWJS_MODULES_DIR",
        "PREVIEWJS_PACKAGE_NAME",
        "PREVIEWJS_PORT"
      ]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "e2e-test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
