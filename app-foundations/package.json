    "axios": "^1.1.3",
    "mobx": "^6.6.1"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "typescript": "4.8.4",
    "unbuild": "0.9.1",
    "vitest": "0.25.2"
